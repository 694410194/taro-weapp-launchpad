{
  "name": "taro-weapp-launchpad",
  "version": "0.1.0",
  "private": true,
  "description": "深圳小王子 xiaowz 出品：面向商业化交付的 Taro 4 微信小程序工程化 Launchpad（React + TypeScript + UnoCSS）。",
  "license": "MIT",
  "author": "深圳小王子 (xiaowz)",
  "packageManager": "pnpm@10.23.0",
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "prepare": "husky",
    "dev:weapp": "taro build --type weapp --watch --env-prefix TARO_",
    "build:weapp": "taro build --type weapp --env-prefix TARO_",
    "create": "taro create",
    "doctor": "taro doctor",
    "clean": "rimraf dist deploy_versions .temp",
    "upload:weapp": "node scripts/upload-weapp.mjs",
    "deploy:weapp": "pnpm build:weapp && pnpm upload:weapp",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.cjs,.mjs --max-warnings=0",
    "lint:fix": "pnpm lint --fix",
    "stylelint": "stylelint \"src/**/*.css\"",
    "stylelint:fix": "pnpm stylelint --fix",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "check": "pnpm lint && pnpm stylelint && pnpm typecheck && pnpm build:weapp"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.4",
    "@tanstack/react-query": "^5.90.12",
    "@tarojs/components": "4.1.9",
    "@tarojs/helper": "4.1.9",
    "@tarojs/plugin-framework-react": "4.1.9",
    "@tarojs/plugin-platform-weapp": "4.1.9",
    "@tarojs/react": "4.1.9",
    "@tarojs/runtime": "4.1.9",
    "@tarojs/shared": "4.1.9",
    "@tarojs/taro": "4.1.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^5.0.9",
    "zod": "^4.2.1"
  },
  "keywords": [
    "taro",
    "weapp",
    "wechat-mini-program",
    "react",
    "typescript",
    "unocss",
    "starter",
    "template"
  ],
  "devDependencies": {
    "@unocss/webpack": "^66.5.10",
    "@babel/core": "^7.24.4",
    "@babel/plugin-transform-class-properties": "7.25.9",
    "@babel/preset-react": "^7.24.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
    "@tarojs/cli": "4.1.9",
    "@tarojs/plugin-generator": "4.1.9",
    "@tarojs/taro-loader": "4.1.9",
    "@tarojs/webpack5-runner": "4.1.9",
    "miniprogram-api-typings": "^4.1.2",
    "miniprogram-ci": "^2.1.26",
    "@types/node": "^25.0.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/webpack-env": "^1.13.6",
    "babel-preset-taro": "4.1.9",
    "eslint": "^8.57.0",
    "eslint-config-taro": "4.1.9",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "less": "^4.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.3.3",
    "react-refresh": "^0.14.0",
    "rimraf": "^6.0.1",
    "stylelint": "^16.4.0",
    "stylelint-config-standard": "^37.0.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.4.5",
    "unocss": "^66.5.10",
    "unocss-applet": "^0.12.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@swc/core",
      "@tarojs/binding",
      "@tarojs/cli",
      "core-js",
      "core-js-pure",
      "esbuild",
      "less",
      "protobufjs"
    ]
  }
}
